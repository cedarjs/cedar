{
  "name": "@cedarjs/prerender",
  "version": "0.0.5",
  "description": "RedwoodJS prerender",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git",
    "directory": "packages/prerender"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browserUtils": {
      "types": "./dist/browserUtils/index.d.ts",
      "default": "./dist/browserUtils/index.js"
    },
    "./detection": {
      "types": "./dist/detection/detection.d.ts",
      "default": "./dist/detection/detection.js"
    }
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx ./build.mts && yarn build:types",
    "build:pack": "yarn pack -o cedarjs-prerender.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.build.json",
    "build:watch": "nodemon --watch src --ext \"js,jsx,ts,tsx,template\" --ignore dist --exec \"yarn build\"",
    "check:attw": "yarn rw-fwtools-attw",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "prepublishOnly": "yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@babel/generator": "7.27.5",
    "@babel/parser": "7.27.5",
    "@babel/traverse": "7.27.4",
    "@cedarjs/babel-config": "workspace:*",
    "@cedarjs/project-config": "workspace:*",
    "@cedarjs/router": "workspace:*",
    "@cedarjs/structure": "workspace:*",
    "@cedarjs/web": "workspace:*",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-replace": "6.0.2",
    "@swc/core": "1.13.2",
    "@whatwg-node/fetch": "0.9.21",
    "babel-plugin-ignore-html-and-css-imports": "0.1.0",
    "cheerio": "1.0.0",
    "graphql": "16.9.0",
    "load-tsconfig": "0.2.5",
    "mime-types": "2.1.35",
    "rollup": "4.24.0",
    "rollup-plugin-swc3": "0.12.1",
    "unimport": "5.0.1",
    "unplugin-auto-import": "19.3.0",
    "vite": "5.4.19",
    "vite-node": "3.2.4"
  },
  "devDependencies": {
    "@cedarjs/framework-tools": "workspace:*",
    "@types/mime-types": "2.1.4",
    "@types/react": "^18.2.55",
    "babel-plugin-tester": "11.0.4",
    "concurrently": "8.2.2",
    "publint": "0.3.12",
    "tsx": "4.20.3",
    "typescript": "5.6.2",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": "19.0.0-rc-f2df5694-20240916",
    "react-dom": "19.0.0-rc-f2df5694-20240916"
  },
  "externals": {
    "react": "react",
    "react-dom": "react-dom"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
