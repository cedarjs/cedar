{
  "name": "@cedarjs/auth-dbauth-middleware",
  "version": "0.0.5",
  "repository": {
    "directory": "packages/auth-providers/dbAuth/middleware",
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsx ./build.mts",
    "build:pack": "yarn pack -o cedarjs-auth-dbauth-middleware.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.build.json",
    "build:types-cjs": "tsc --build --verbose tsconfig.cjs.json",
    "check:attw": "yarn attw -P",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.js",
        "types": "./dist/cjs/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cedarjs/auth-dbauth-api": "workspace:*",
    "@cedarjs/web": "workspace:*"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.1",
    "@cedarjs/api": "workspace:*",
    "@cedarjs/framework-tools": "workspace:*",
    "@cedarjs/graphql-server": "workspace:*",
    "@types/aws-lambda": "8.10.145",
    "concurrently": "8.2.2",
    "publint": "0.3.12",
    "ts-toolbelt": "9.6.0",
    "tsx": "4.20.3",
    "typescript": "5.6.2",
    "vitest": "3.2.4"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
