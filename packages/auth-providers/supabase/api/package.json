{
  "name": "@cedarjs/auth-supabase-api",
  "version": "0.0.5",
  "repository": {
    "directory": "packages/auth-providers/supabase/api",
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    ".": {
      "default": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./dist/decoder": {
      "default": {
        "default": "./dist/decoder.js",
        "types": "./dist/decoder.d.ts"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx ./build.mts && yarn build:types",
    "build:pack": "yarn pack -o cedarjs-auth-supabase-api.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.json",
    "build:watch": "nodemon --watch src --ext \"js,jsx,ts,tsx,template\" --ignore dist --exec \"yarn build\"",
    "check:attw": "yarn attw -P",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@cedarjs/api": "workspace:*",
    "@supabase/ssr": "0.5.1",
    "jsonwebtoken": "9.0.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.1",
    "@cedarjs/framework-tools": "workspace:*",
    "@types/aws-lambda": "8.10.145",
    "@types/jsonwebtoken": "9.0.10",
    "concurrently": "8.2.2",
    "publint": "0.3.12",
    "tsx": "4.20.3",
    "typescript": "5.6.2",
    "vitest": "3.2.4"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
