{
  "name": "@cedarjs/vite",
  "version": "2.4.1",
  "description": "Vite configuration package for CedarJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git",
    "directory": "packages/vite"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./api": {
      "types": "./dist/api/vite-plugin-cedar-vitest-api-preset.d.ts",
      "default": "./dist/api/vite-plugin-cedar-vitest-api-preset.js"
    },
    "./client": {
      "require": "./dist/cjs/client.js",
      "import": "./dist/client.js"
    },
    "./buildFeServer": {
      "require": "./dist/cjs/buildFeServer.js",
      "import": "./dist/buildFeServer.js"
    },
    "./build": {
      "import": "./dist/build/build.js",
      "default": "./dist/cjs/build/build.js"
    },
    "./bins/rw-vite-build.mjs": "./bins/rw-vite-build.mjs"
  },
  "bin": {
    "rw-dev-fe": "./dist/devFeServer.js",
    "rw-serve-fe": "./dist/runFeServer.js",
    "rw-vite-build": "./bins/rw-vite-build.mjs",
    "rw-vite-dev": "./bins/rw-vite-dev.mjs",
    "vite": "./bins/vite.mjs"
  },
  "files": [
    "dist",
    "inject"
  ],
  "scripts": {
    "build": "tsx build.ts && yarn build:types",
    "build:pack": "yarn pack -o cedarjs-vite.tgz",
    "build:types": "tsc --build --verbose ./tsconfig.build.json",
    "check:attw": "tsx ./attw.ts",
    "check:package": "concurrently npm:check:attw yarn:publint",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@ast-grep/napi": "0.40.5",
    "@babel/generator": "7.28.6",
    "@babel/parser": "7.28.6",
    "@babel/traverse": "7.28.6",
    "@cedarjs/auth": "2.4.1",
    "@cedarjs/babel-config": "2.4.1",
    "@cedarjs/cookie-jar": "2.4.1",
    "@cedarjs/internal": "2.4.1",
    "@cedarjs/project-config": "2.4.1",
    "@cedarjs/server-store": "2.4.1",
    "@cedarjs/testing": "2.4.1",
    "@cedarjs/web": "2.4.1",
    "@swc/core": "1.15.10",
    "@vitejs/plugin-react": "4.7.0",
    "@whatwg-node/fetch": "0.10.13",
    "@whatwg-node/server": "0.10.18",
    "acorn-loose": "8.5.2",
    "buffer": "6.0.3",
    "busboy": "^1.6.0",
    "cookie": "1.1.1",
    "core-js": "3.48.0",
    "dotenv-defaults": "5.0.2",
    "execa": "5.1.1",
    "express": "4.22.1",
    "find-my-way": "8.2.2",
    "http-proxy-middleware": "3.0.5",
    "isbot": "5.1.34",
    "react": "18.3.1",
    "react-server-dom-webpack": "19.2.3",
    "rimraf": "6.1.2",
    "vite": "5.4.21",
    "vite-plugin-cjs-interop": "2.4.0",
    "vite-plugin-node-polyfills": "0.24.0",
    "ws": "8.19.0",
    "yargs-parser": "21.1.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@hyrious/esbuild-plugin-commonjs": "0.2.6",
    "@types/busboy": "^1",
    "@types/express": "4",
    "@types/react": "^18.2.55",
    "@types/ws": "^8",
    "@types/yargs-parser": "21.0.3",
    "concurrently": "9.2.1",
    "glob": "11.1.0",
    "memfs": "4.56.10",
    "publint": "0.3.17",
    "rollup": "4.57.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
