{
  "name": "@cedarjs/codemods",
  "version": "2.3.0",
  "description": "Codemods to ease upgrading a CedarJS Project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedarjs/cedar.git",
    "directory": "packages/codemods"
  },
  "license": "MIT",
  "type": "commonjs",
  "bin": "./dist/codemods.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx ./build.mts",
    "build:pack": "yarn pack -o cedarjs-codemods.tgz",
    "build:watch": "nodemon --watch src --ignore dist --exec \"yarn build\"",
    "check:package": "yarn publint",
    "generate:codemod": "yarn node ./tasks/generateCodemod/generateCodemod.mjs",
    "prepublishOnly": "yarn build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "^7.26.10",
    "@babel/parser": "7.28.5",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/runtime-corejs3": "7.28.4",
    "@babel/traverse": "7.28.5",
    "@cedarjs/project-config": "workspace:*",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@vscode/ripgrep": "1.17.0",
    "@whatwg-node/fetch": "0.10.13",
    "cheerio": "1.1.2",
    "core-js": "3.47.0",
    "deepmerge": "4.3.1",
    "execa": "5.1.1",
    "fast-glob": "3.3.3",
    "graphql": "16.12.0",
    "jscodeshift": "17.0.0",
    "pascalcase": "1.0.0",
    "prettier": "3.6.2",
    "tasuku": "2.0.5",
    "typescript": "5.9.3",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@cedarjs/framework-tools": "workspace:*",
    "@types/babel__core": "7.20.5",
    "@types/jscodeshift": "0.12.0",
    "@types/yargs": "17.0.35",
    "publint": "0.3.16",
    "tsx": "4.21.0",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
